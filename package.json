{
  "name": "bitphantom-waf",
  "version": "1.0.0",
  "description": "Modular plugin-based Web Application Firewall with AI-powered threat detection",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "start:pm2": "pm2 start ecosystem.config.js --env production",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:pentest": "node tests/automated-pentest.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "docker:build": "docker build -t bitphantom-waf .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "demo": "node examples/demo.js",
    "dashboard": "open http://localhost:3001/waf-admin",
    "security": "npm audit --audit-level=moderate",
    "build": "echo 'No build step'"
  },
  "keywords": [
    "waf",
    "firewall",
    "security",
    "xss",
    "sqli",
    "csrf",
    "ai",
    "plugin"
  ],
  "author": "BitPhantom",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.13.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "node-fetch": "^2.6.11"
  },
  "optionalDependencies": {
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "winston": "^3.8.2",
    "rotating-file-stream": "^3.1.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.0",
    "express-rate-limit": "^6.7.0",
    "ioredis": "^5.3.2",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.42.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}